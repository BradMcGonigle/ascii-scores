{
  "name": "ascii-scores-monorepo",
  "version": "0.20.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @ascii-scores/web dev",
    "dev:cli": "pnpm --filter ascii-scores-cli dev",
    "build": "pnpm -r build",
    "build:web": "pnpm --filter @ascii-scores/web build",
    "build:cli": "pnpm --filter ascii-scores-cli build",
    "build:core": "pnpm --filter @ascii-scores/core build",
    "start": "pnpm --filter @ascii-scores/web start",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "test": "echo \"No tests configured yet\" && exit 0"
  },
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=20.0.0"
  }
}
